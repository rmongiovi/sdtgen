IDENT pager1;
TITLE "Grammar G2 from A Practical General Method of Constructing LR(k) Parsers";

OPTIONS
   shiftreduce, splitstates;

SCANNER
   "a";
   "b";
   "c";
   "d";
   "e";
   "t";
   "u";

PARSER
   <X> -->
	"a" <Y> "d"
      | "a" <Z> "c"
      | "a" <T>
      | "b" <Y> "e"
      | "b" <Z> "d"
      | "b" <T>
   ;

   <Y> -->
	"t" <W>
      | "u" <X>
   ;

   <Z> -->
	"t" "u"
   ;

   <T> -->
	"u" <X> "a"
   ;

   <W> -->
	"u" <V>
   ;

   <V> -->
	""
   ;
